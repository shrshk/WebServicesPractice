package com.shirishProgramming;

import com.shirishProgramming.HelloWorld;
import com.shirishProgramming.Override;
import com.shirishProgramming.WebService;

//Service Implementation
@WebService(endpointInterface = "com.shirishProgramming.HelloWorld")
public class HelloWorldImpl implements HelloWorld{

	@Override
	public String getHelloWorldAsString(String name) {
		return "Hello World JAX-WS " + name;
	}

}
